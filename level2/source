#include <stdio.h>

void p()
{
    void *ret; // EBP - 12
    char result[76]; // EBP - 76

    fflush(stdout);
    gets((char *)result);
    ret = NULL; // = EBP + 4 (pointer saved by 'call' in main)
    if (((int)ret & 0xb0000000) == 0xb0000000)
    {
        printf("(%p)\n", ret);
        exit(1);
    }
    puts(result);
    strdup(result);
}

int main(int argc, char const *argv[])
{
    p();
    return 0;
}
